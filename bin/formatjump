#!/bin/bash

len=$1

case $len in
	2)
		findextra=
		replextra='  '
		data='.da'
		bank='\1'
		;;
	3)
		findextra='\(..\)'
		replextra='\5'
		data='.dla'
		bank='\5'
		;;
	*)
		echo length $len not supported 1>&2
		exit 1
esac

echo
echo '; jumptable'
sed 's,^\(..\)\(....\)    \(..\)\(..\)'"$findextra"'.*,\1\2    \3\4'"$replextra"'       '"$data $bank"'\4\3,'
echo
